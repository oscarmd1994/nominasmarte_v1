<link rel="stylesheet" href="~/Scripts/jqxGrid/jqwidgets/styles/jqx.base.css" type="text/css" />

<div class="container bg-white">
  @*Botones Flotantes*@

  <div class="container-fluid">
    <div class="row mt-4 border-left-primary p-3 rounded shadow border-right-primary">
      <div class="tab-pane fade show p-4 row table-responsive" id="navXMLS" role="tabpanel" aria-labelledby="nav-Xml-tab">
        <nav>
          <div class="nav nav-tabs " id="nav-tab" role="tablist">
            <a class="nav-item nav-link  font-navs  " id="nav-xmlnor-tab" data-toggle="tab" href="#nav-XmlNo" role="tab" aria-controls="nav-Xml-tab">
              <i class="far fa-file-code col-ico mr-ico-05"></i> Xml Normal
            </a>
            <a class="nav-item nav-link font-navs" id="nav-xmlFi-tab" data-toggle="tab" href="#nav-XmlFi" role="tab" aria-controls="nav-Xml-tab" pagPre="false">
              <i class="far fa-file-code col-ico mr-ico-05 "></i> Xml Finiquito
            </a>
          </div>
        </nav>
        <div class="tab-content" id="nav-tabContent">
          <!-- pantalla de Xml tab-->
          <div class="tab-pane fade show  p-4" id="nav-XmlNo" role="tabpanel" aria-labelledby="nav-XMLS">
            <div class="row mt-4 ">
              <div id="div-most-alert-data-gen" class="col-md-8 offset-3"></div>
            </div>
            <div class="col-md-10 offset-2" style="margin-left:10%">
              <div class="form-group text-center rounded shadow p-3 border-left-primary">
                <div class="col-md-12">
                  <div class="form-group row col-md-6" style="margin-left:2%">
                  </div>
                  <div class="col-md-12">
                    <hr />
                  </div>

                  <div class="row mt-6">

                    <div class="col-md-6">
                      <div class="form-group row" style="margin-left:2%">
                        <label for="EmpresaNom" class="col-sm-4 col-form-label font-labels">
                          <i class="fas fa-building mr-2 col-ico"></i>
                          Empresa:
                        </label>
                        <div class="col-sm-6">
                          <select placeholder="Empresa" tp-select="EmpresaNom" class="form-control form-control-sm" id="EmpresaNom">
                            <option value="0">Selecciona</option>
                          </select>
                        </div>
                      </div>
                      <div class="form-group row" style="margin-left:2%">
                        <label for="anoNom" class="col-sm-4 col-form-label font-labels">
                          <i class="fas fa-calendar mr-2 col-ico"></i>
                          Año:
                        </label>
                        <div class="col-sm-6">
                          <input placeholder="Año" maxlength="4" tvvisible="false" type="text" id="anoNom" class="form-control form-control-sm text-capitalize" />
                        </div>
                      </div>
                      <div class="form-group row" style="margin-left:2%">
                        <label for="TipoPerido" class="col-sm-4 col-form-label font-labels">
                          <i class="fas fa-pallet mr-2 col-ico"></i>
                          Tipo de Periodo:
                        </label>
                        <div class="col-sm-6">
                          <select placeholder="Tipo de Periodo" tp-select="TipoPerido" class="form-control form-control-sm" id="TipodePerdioRec">
                            <option value="0">Selecciona</option>
                          </select>
                        </div>
                      </div>
                      <div class=" custom-switch">
                        <input type="checkbox" class="custom-control-input" id="CheckRecib2" style="visibility:hidden">
                        <label class="custom-control-label" for="CheckRecib2" id="LaCheckRecib2" style="visibility:hidden">
                          XML Recibo 2
                        </label>
                      </div>

                      <div class=" custom-switch">
                        <input type="checkbox" class="custom-control-input" id="CheckRecib3" style="visibility:hidden">
                        <label class="custom-control-label" for="CheckRecib3" id="LaCheckRecib3" style="visibility:hidden">
                          XML Recibo 3
                        </label>
                      </div>

                    </div>
                    <div class="col-md-6">
                      <div class="form-group row" style="margin-left:-25%">
                        <label for="peridoNom" class="col-sm-4 col-form-label font-labels">
                          <i class="fas fa-pager mr-2 col-ico"></i>
                          Periodo:
                        </label>
                        <div class="col-sm-9" style="margin-left:-10%">
                          <select placeholder="Perido" tp-select="PeridoNom" class="form-control form-control-sm" id="PeridoNom">
                            <option value="0">Selecciona</option>
                          </select>
                        </div>
                        <label for="FechaPeriodo" class="col-sm-12 col-form-label font-labels" id="FechaPeridod"></label>
                      </div>

                      <div class=" custom-switch">
                        <input type="checkbox" class="custom-control-input" id="CheckPDF" style="visibility:visible">
                        <label class="custom-control-label" for="CheckPDF" id="LaCheckPDF" style="visibility:visible">
                          Crear Recibos PDF
                        </label>
                      </div>

                      <div class="custom-control custom-switch" style="margin-top:15px;margin-left:-10%;">
                        <input type="checkbox" class="custom-control-input" id="CheckXEmpleado" style="visibility:visible">
                        <label class="custom-control-label" for="CheckXEmpleado" id="LaCheckXEmpleado" style="visibility:visible">
                          Por empleado
                        </label>
                      </div>

                      <div class="form-group row" id="dropbusemple" style="visibility:hidden">
                        <label for="EmpleadoNom" class="col-sm-12 col-form-label font-labels">
                          <i class="fas fa-users mr-2 col-ico"></i>
                          Empleados:
                        </label>
                        <div class="col-sm-12" style="margin-left:-5%">
                          <div id='content'>
                            <i class="fas fa-search mr-2 col-ico"></i>
                            <input id="jqxInput" />
                          </div>
                        </div>
                      </div>
                      <div class="form-group row">
                        <div id="jqxLoader"></div>
                      </div>

                    </div>
                  </div>
                </div>
                <div class="col-md-12">

                </div>
              </div>
              <center>
                <div style="margin-left:20%">
                  <div class="form-group row">
                    <div id="jqxLoader2"></div>
                  </div>
                </div>
              </center>
            </div>
          </div>
          <!-- pantalla de vista xml finiquito tab-->
          <div class="tab-pane fade p-4" id="nav-XmlFi" role="tabpanel" aria-labelledby="nav-XMLS">
            <div class="row mt-4 ">
              <div id="div-most-alert-data-gen" class="col-md-8 offset-3"></div>
            </div>
            <div class="col-md-10 offset-2" style="margin-left:10%">
              <div class="form-group text-center rounded shadow p-3 border-left-primary">
                <div class="col-md-12">
                  <div class="form-group row col-md-6" style="margin-left:2%">
                  </div>
                  <div class="col-md-12">
                    <hr />
                  </div>
                  <div class="row mt-6">
                    <div class="col-md-6">
                      <div class="form-group row" style="margin-left:2%">
                        <label for="EmpresaNomFi" class="col-sm-4 col-form-label font-labels">
                          <i class="fas fa-building mr-2 col-ico"></i>
                          Empresa:
                        </label>
                        <div class="col-sm-6">
                          <select placeholder="Empresa" tp-select="EmpresaNomFi" class="form-control form-control-sm" id="EmpresaNomFi">
                            <option value="0">Selecciona</option>
                          </select>
                        </div>
                      </div>
                      <div class="form-group row" style="margin-left:2%">
                        <label for="anoNomFi" class="col-sm-4 col-form-label font-labels">
                          <i class="fas fa-calendar mr-2 col-ico"></i>
                          Año:
                        </label>
                        <div class="col-sm-6">
                          <input placeholder="Año" maxlength="4" tvvisible="false" type="text" id="anoNomFi" class="form-control form-control-sm text-capitalize" />
                        </div>
                      </div>
                      <div class="form-group row" style="margin-left:2%">
                        <label for="TipoPeridoFi" class="col-sm-4 col-form-label font-labels">
                          <i class="fas fa-pallet mr-2 col-ico"></i>
                          Tipo de Periodo:
                        </label>
                        <div class="col-sm-6">
                          <select placeholder="Tipo de Periodo" tp-select="TipoPerido" class="form-control form-control-sm" id="TipodePerdioRecFi">
                            <option value="0">Selecciona</option>
                          </select>
                        </div>
                      </div>
                    </div>
                    <div class="col-md-6">
                      <div class="form-group row" style="margin-left:-25%">
                        <label for="peridoNom" class="col-sm-4 col-form-label font-labels">
                          <i class="fas fa-pager mr-2 col-ico"></i>
                          Periodo:
                        </label>
                        <div class="col-sm-9" style="margin-left:-10%">
                          <select placeholder="Perido" tp-select="PeridoNom" class="form-control form-control-sm" id="PeridoNomFi">
                            <option value="0">Selecciona</option>
                          </select>
                        </div>
                        <label for="FechaPeriodo" class="col-sm-12 col-form-label font-labels" id="FechaPeridodFi"></label>
                      </div>
                      <div class="form-group row" id="dropbusempleFi" style="visibility:visible">
                        <label for="EmpleadoNom" class="col-sm-12 col-form-label font-labels">
                          <i class="fas fa-users mr-2 col-ico"></i>
                          Empleados:
                        </label>
                        <div class="col-sm-12" style="margin-left:-5%">
                          <div id='content'>
                            <i class="fas fa-search mr-2 col-ico"></i>
                            <input id="jqxInputFi" />
                          </div>
                        </div>
                      </div>
                      <div class="form-group row">
                        <div id="jqxLoader"></div>
                      </div>
                      <div class="form-group row">
                        <div id="jqxLoader2"></div>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="col-md-6">
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  @*Botones de flotantes*@
  <div>
    <button class="flotante2 btn btn-primary btn-circle shadow btn-priority" title="Limpiar todos los campos" id="btnFloLimpiar" type="button" btvisible="true" value="1">
      <i class="fas fa-eraser"></i>
    </button>
    <button class="flotante3 btn btn-primary btn-circle shadow btn-priority" title="Generar PDF masivos" id="btnPDFms" type="button" btvisible="true" value="0" style="visibility:hidden">
      <i class="far fa-file-pdf"></i>
    </button>
    <button class="flotante3 btn btn-primary btn-circle shadow btn-priority" title="Generar Xml masivos" id="btnXmlms" type="button" btvisible="true" value="1" style="visibility:visible">
      <i class="far fa-file-code"></i>
    </button>
    <button class="flotante4 btn btn-primary btn-circle shadow btn-priority" title="Dowland" id="btnDowlan" type="button" btvisible="true" value="1" style="visibility:hidden">
      <i class="fas fa-download"></i>
    </button>
  </div>
</div>

<script src="~/Scripts/main/XMLs.js?n=0.4"></script>
