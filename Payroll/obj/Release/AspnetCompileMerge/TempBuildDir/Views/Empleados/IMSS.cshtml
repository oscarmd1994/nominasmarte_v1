<div class="container bg-white pb-1 rounded p-3">
    @*<div class="tab-pane fade p-3" id="nav-imss" role="tabpanel" aria-labelledby="nav-imss-tab">*@
        <div class="row mt-4">
            <div id="div-most-alert-data-imss" class="col-md-6 offset-3"></div>
            <div class="col-md-12 mb-4">
                <div class="text-left">
                    <h6 class="col-ico">
                        <i class="fas fa-circle fa-sm mr-ico-05"></i>
                        Registros oficiales
                    </h6>
                    <hr />
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group row">
                    <input type="hidden" readonly class="form-control form-control-sm" id="clvimss" />
                    <label for="regimss" class="col-sm-4 col-form-label font-labels">
                        Registro IMSS:
                    </label>
                    <div class="col-sm-8">
                        <input placeholder="Numero de registro" maxlength="11" type="text" id="regimss" class="form-control form-control-sm" />
                    </div>
                </div>
                <div class="form-group row">
                    <label for="rfc" class="col-sm-4 col-form-label font-labels">
                        R.F.C:
                    </label>
                    <div class="col-sm-8">
                        <input placeholder="R.F.C" maxlength="13" type="text" id="rfc" class="form-control form-control-sm text-uppercase" />
                        <div class="invalid-feedback">
                            Los 10 primeros caracteres de tu rfc deben de concordar con los primeros 10 caracteres de tu curp.
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group row">
                    <label for="curp" class="col-sm-4 col-form-label font-labels">
                        C.U.R.P:
                    </label>
                    <div class="col-sm-8">
                        <input placeholder="C.U.R.P" type="text" id="curp" class="form-control form-control-sm text-uppercase" maxlength="18" />
                        <div class="invalid-feedback">
                            Introduzca una curp valida, compruebe que los primeros 10 digitos del rfc concuerden con los de la curp.
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row mt-4">
            <div class="col-md-12 mb-4">
                <div class="text-left">
                    <h6 class="col-ico">
                        <i class="fas fa-circle fa-sm mr-ico-05"></i>
                        Otros
                    </h6>
                    <hr />
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group row">
                    <label for="nivest" class="col-sm-4 col-form-label font-labels">
                        Nivel de estudios:
                    </label>
                    <div class="col-sm-8">
                        <select id="nivest" tp-select="Nivel de estudios" class="form-control form-control-sm">
                            <option value="0">Selecciona</option>
                        </select>
                    </div>
                </div>
                <div class="form-group row">
                    <label for="nivsoc" class="col-sm-4 col-form-label font-labels">
                        Nivel socioeconomico:
                    </label>
                    <div class="col-sm-8">
                        <select id="nivsoc" tp-select="Nivel socioeconomico" class="form-control form-control-sm">
                            <option value="0">Selecciona</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="col-md-6 text-center">
                <img width="250" class="img-fluid" src="~/Content/img/form-complete.svg" alt="data-register">
            </div>
            <div class="offset-md-6 col-md-6 mt-5 text-center">
                <button class="btn btn-block btn-light col-btn-login font-button" id="btn-save-data-imss">
                    Continuar <i class="fas fa-arrow-right ml-ico-05"></i>
                </button>
            </div>
        </div>
    @*</div>*@
</div>
<script src="~/Scripts/main/main.js"></script>