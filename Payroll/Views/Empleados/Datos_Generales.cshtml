<script type="text/javascript">
    $(function () {
        setTimeout(() => {
            const getDataTabNom1 = JSON.parse(localStorage.getItem('objectDataTabNom'));
            if (JSON.parse(localStorage.getItem('objectDataTabNom')) != null) {
                for (i in getDataTabNom1) {
                    if (getDataTabNom1[i].key == "nom") {
                        if (getDataTabNom1[i].data.tippag == 219 || getDataTabNom1[i].data.tippag == 221 || getDataTabNom1[i].data.tippag == 220) {
                            document.getElementById('banuse').removeAttribute("disabled");
                            document.getElementById('cunuse').removeAttribute("disabled");
                            document.getElementById('cunuse').value = getDataTabNom1[i].data.cunuse;
                        } else {
                            document.getElementById('banuse').disabled = true;
                            document.getElementById('cunuse').disabled = true;
                        }
                        if (getDataTabNom1[i].data.banuse != "") {
                            document.getElementById('banuse').value = getDataTabNom1[i].data.banuse;
                        }
                    }
                }
            }
            const getDataGenTab = JSON.parse(localStorage.getItem('objectTabDataGen'));
            if (JSON.parse(localStorage.getItem('objectTabDataGen')) != null) {
                for (i in getDataGenTab) {
                    if (getDataGenTab[i].key == "general") {
                        if (getDataGenTab[i].data.codpost != "") {
                            if (document.getElementById('btn-verif-codpost') != null) {
                                document.getElementById('btn-verif-codpost').removeAttribute("disabled");
                                document.getElementById('btn-verif-codpost').classList.add("btn-info");
                            }
                        } else {
                            document.getElementById('btn-verif-codpost').disabled = true;
                        }
                    }
                }
            }
        }, 3000);
        //fchangetippag();
        setTimeout(() => { fchangetippag(); }, 2000);
    });
    function check(e) {
        tecla = (document.all) ? e.keyCode : e.which;
        if (tecla == 8) {
            return true;
        }
        patron = /[A-Za-z0-9 ]/;
        tecla_final = String.fromCharCode(tecla);
        return patron.test(tecla_final);
    }
</script>

