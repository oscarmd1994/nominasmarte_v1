<div class="container bg-white pb-1 rounded p-3">
    @*<div class="tab-pane fade p-3" id="nav-datanom" role="tabpanel" aria-labelledby="nav-datanom-tab">*@
        <div class="row mt-4">
            <div id="div-most-alert-data-nomina" class="col-md-6 offset-3"></div>
            <div class="col-md-12 mb-4">
                <div class="text-left">
                    <h6 class="col-ico">
                        <i class="fas fa-circle fa-sm mr-ico-05"></i>
                        Datos de ingreso
                    </h6>
                    <hr />
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group row">
                    <input type="hidden" readonly class="form-control form-control-sm" id="clvnom" />
                    <label for="numnom" class="col-sm-4 col-form-label font-labels">
                        N°. Nómina:
                    </label>
                    <div class="col-sm-8">
                        <div class="input-group mb-2">
                            <div class="input-group-prepend">
                                <div style="cursor:pointer !important;" class="input-group-text" id="btn-search-num-nomina"> <i class="fas fa-search"></i> </div>
                            </div>
                            <input type="number" placeholder="Numero de nómina" id="numnom" class="form-control form-control-sm" />
                        </div>
                    </div>
                </div>
                <div class="form-group row">
                    <label for="salmen" class="col-sm-4 col-form-label font-labels">
                        Salario mensual:
                    </label>
                    <div class="col-sm-8">
                        <div class="input-group mb-2">
                            <div class="input-group-prepend">
                                <div class="input-group-text"> <i class="fas fa-dollar-sign"></i> </div>
                            </div>
                            <input type="number" placeholder="Salario mensual" id="salmen" class="form-control form-control-sm" />
                        </div>
                    </div>
                </div>
                <div class="form-group row">
                    <label for="pagret" class="col-sm-4 col-form-label font-labels">
                        Con pago retroactivo:
                    </label>
                    <div class="col-sm-8">
                        <select tp-select id="pagret" class="form-control form-control-sm">
                            <option value="0">Selecciona</option>
                            <option value="SI">SI</option>
                            <option selected value="NO">NO</option>
                        </select>
                    </div>
                </div>
                <div class="form-group row">
                    <label for="tipemp" class="col-sm-4 col-form-label font-labels">
                        Tipo de empleado:
                    </label>
                    <div class="col-sm-8">
                        <select id="tipemp" tp-select="Tipo de empleado" class="form-control form-control-sm">
                            <option value="0">Selecciona</option>
                        </select>
                    </div>
                </div>
                <div class="form-group row">
                    <label for="tipmon" class="col-sm-4 col-form-label font-labels">
                        Tipo de moneda:
                    </label>
                    <div class="col-sm-8">
                        <input type="text" readonly id="tipmon" class="form-control form-control-sm" value="MONEDA NACIONAL" />
                    </div>
                </div>
                <div class="form-group row">
                    <label for="nivemp" class="col-sm-4 col-form-label font-labels">
                        Nivel empleado:
                    </label>
                    <div class="col-sm-8">
                        <select id="nivemp" tp-select="Nivel empleado" class="form-control form-control-sm">
                            <option value="0">Selecciona</option>
                        </select>
                    </div>
                </div>
                <div class="form-group row">
                    <label for="tipjor" class="col-sm-4 col-form-label font-labels">
                        Tipo de jornada:
                    </label>
                    <div class="col-sm-8">
                        <select id="tipjor" tp-select="Tipo de jornada" class="form-control form-control-sm">
                            <option value="0">Selecciona</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group row">
                    <label for="tipcon" class="col-sm-4 col-form-label font-labels">
                        Tipo de contrato:
                    </label>
                    <div class="col-sm-8">
                        <select id="tipcon" tp-select="Tipo de contrato" class="form-control form-control-sm">
                            <option value="0">Selecciona</option>
                        </select>
                    </div>
                </div>
                <div class="form-group row">
                    <label for="fecing" class="col-sm-4 col-form-label font-labels">
                        Fecha de ingreso:
                    </label>
                    <div class="col-sm-8">
                        <input type="date" placeholder="Fecha de ingreso" tp-date="less" id="fecing" class="form-control form-control-sm" />
                    </div>
                </div>
                <div class="form-group row">
                    <label for="fecant" class="col-sm-4 col-form-label font-labels">
                        Fecha de reconocimiento de antiguedad:
                    </label>
                    <div class="col-sm-8">
                        <input type="date" placeholder="Fecha de reconocimiento de antiguedad" tp-date="less" id="fecant" class="form-control form-control-sm" />
                    </div>
                </div>
                <div class="form-group row">
                    <label for="vencon" class="col-sm-4 col-form-label font-labels">
                        Vencimiento contrato:
                    </label>
                    <div class="col-sm-8">
                        <input type="date" tp-date="higher" placeholder="Vencimiento contrato" id="vencon" class="form-control form-control-sm" />
                    </div>
                </div>
                <div class="form-group row">
                    <label for="estats" class="col-sm-4 col-form-label font-labels">
                        Tipo de Alta:
                    </label>
                    <div class="col-sm-8">
                        <select id="estats" tp-select="Tipo de alta" class="form-control form-control-sm">
                            <option selected value="0">Selecciona</option>
                            <option value="Alta">ALTA</option>
                            <option value="Reingreso">REINGRESO</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="offset-6 col-md-6 text-center mt-4">
                <img width="250" class="img-fluid" src="~/Content/img/form-complete.svg" alt="data-register">
            </div>
            <div class="offset-md-6 col-md-6 mt-5 text-center">
                <button class="btn btn-block btn-light col-btn-login font-button" id="btn-save-data-nomina">
                    Continuar <i class="fas fa-arrow-right ml-ico-05"></i>
                </button>
            </div>
        </div>
    @*</div>*@
</div>
<script src="~/Scripts/main/main.js"></script>