                      <i class="fas fa-file-invoice-dollar mr-2 col-ico"></i>Recibo:</label>
                     <div class="col-sm-8">
                        <select placeholder="Recibo" tp-select="ReciboTim" class="form-control form-control-sm" id="ReciboTim">
                            <option value="0">Selecciona</option>
                            <option value="1">Recibo 1 Asimilado</option>
                            <option value="2">Recibo 2 Asimilado</option>
                            <option value="3">Aguinaldo</option>
                            <option value="5">Transporte </option>
                        </select>
                    </div>
                </div>*@


                <div class="form-group row col-md-8">
                    <div class="custom-file">
                        <input type="file" class="custom-file-input" id="fileUpload" lang="en">
                        <label class="custom-file-label" for="customFileLang">Seleccionar Archivo</label>
                    </div>
                </div>
                <div class="form-group row col-md-9" style="margin-left:20%">
                    <button type="submit" class="btn btn-primary mb-2 btn-priority" id="btnTiembrar"><span class="fa fa-arrow-circle-o-up"></span> Generar recibo timbrado </button>
                </div>

